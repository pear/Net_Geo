<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<!-- do not use the "Type" attribute here, that one is only for
     generated package.xml files -->
<package version="1.0">
  <name>Net_Geo</name>
  <summary>Geographical locations based on Internet address</summary>
  <description>
    Obtains geogrphical information based on IP number, domain name,
    or AS number. Makes use of CAIDA Net_Geo lookup or locaizer extension.
  </description>
  <maintainers>
    <maintainer>
      <user>graeme</user>
      <name>Graeme Merrall</name>
      <email>graeme@inetix.com.au</email>
    </maintainer>
    </maintainers>
    <release>
        <license>PHP</license>
        <version>1.x.x</version>
        <date>2005-xx-xx</date>
        <state>stable</state>
        <notes>
        </notes>
    <filelist>
      <dir name="/" baseinstalldir="Net">
                <file role="php">Geo.php</file>
                <file role="doc">README</file>
      </dir>
    </filelist>
    <deps>
     <dep type="pkg" rel="has">PEAR</dep>
     <dep type="pkg" rel="has">Cache</dep>
    </deps>
    </release>
</package>
